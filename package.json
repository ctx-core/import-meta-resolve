{
	"name": "@ctx-core/import-meta-resolve",
	"version": "1.1.10",
	"type": "module",
	"files": [
		"*.d.ts"
	],
	"types": "./index.d.ts",
	"exports": {
		".": {
			"default": "./index.mjs",
			"import": "./index.mjs",
			"require": "./index.cjs"
		},
		"./package.json": "./package.json"
	},
	"dependencies": {
		"import-meta-resolve": "^4.0.0"
	},
	"devDependencies": {
		"c8": "^9.1.0",
		"check-dts": "^0.7.2",
		"tsx": "^4.7.1",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"noUpdate": [],
	"homepage": "https://github.com/ctx-core/import-meta-resolve#readme",
	"svelte": "./lib/index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/import-meta-resolve.git"
	},
	"bugs": {
		"url": "https://github.com/ctx-core/import-meta-resolve/issues"
	},
	"sideEffects": false,
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"test": "pnpm test:unit && pnpm test:types",
		"test:types": "check-dts",
		"test:unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test:unit:coverage": "c8 pnpm test:unit"
	}
}