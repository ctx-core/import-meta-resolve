{
	"name": "@ctx-core/import-meta-resolve",
	"version": "1.1.12",
	"description": "ctx-core import-meta-resolve",
	"keywords": [
		"import",
		"import-meta",
		"import-meta-resolve",
		"ctx-core"
	],
	"homepage": "https://github.com/ctx-core/import-meta-resolve#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/import-meta-resolve/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/import-meta-resolve.git"
	},
	"license": "Apache-2.0",
	"author": {
		"name": "Brian Takita",
		"url": "https://briantakita.me",
		"email": "info+ctx-core-import-meta-resolve@briantakita.me"
	},
	"type": "module",
	"files": [
		"*.d.ts"
	],
	"types": "./index.d.ts",
	"exports": {
		".": {
			"default": "./index.mjs",
			"import": "./index.mjs",
			"require": "./index.cjs"
		},
		"./package.json": "./package.json"
	},
	"dependencies": {
		"import-meta-resolve": "^4.1.0"
	},
	"devDependencies": {
		"c8": "^10.1.2",
		"check-dts": "^0.8.0",
		"tsx": "^4.16.0",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"noUpdate": [],
	"svelte": "./lib/index.js",
	"sideEffects": false,
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"test": "pnpm run /^test:/",
		"test:types": "check-dts",
		"test:unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"disable:test:unit:coverage": "c8 pnpm test:unit"
	}
}